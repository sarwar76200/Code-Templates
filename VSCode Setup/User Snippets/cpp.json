			"        for (int j = 1; j <= n; ++j) {// 1-based graph",
			"            int p = up[j][i - 1];",
			"            if (p == -1) continue;",
			"            up[j][i] = up[p][i - 1];",
			"        }",
			"    }",
			"}",
			"",
			"int get_lca(int u, int v) {",
			"    if (dist[u] > dist[v])",
			"        swap(u, v);",
			"",
			"    int dif = dist[v] - dist[u];",
			"    while (dif > 0) {",
			"        int lg = __lg(dif);",
			"        v = up[v][lg];",
			"        dif -= (1 << lg);",
			"    }",
			"    if (u == v)",
			"        return u;",
			"",
			"    for (int i = lg - 1; i >= 0; --i) {",
			"        if (up[u][i] == up[v][i]) continue;",
			"        u = up[u][i];",
			"        v = up[v][i];",
			"    }",
			"    return up[u][0];",
			"}",
			"",
			"int get_kth_ancestor(int v, int k) {",
			"    while (k > 0) {",
			"        int lg = __lg(k);",
			"        v = up[v][lg];",
			"        k -= (1 << lg);",
			"    }",
			"    return v;",
			"}",
