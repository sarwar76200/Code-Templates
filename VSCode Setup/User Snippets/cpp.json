			"  int n, lg;",
			"  function<Tp(Tp, Tp)> op;",
			"",
			"  explicit sparse(const vector<Tp>& a, const function<Tp(Tp, Tp)>& op = [](Tp x, Tp y) -> Tp { return min(x, y); }) {",
			"    sparse::n = (int) a.size();",
			"    sparse::lg = __lg(sparse::n) + 1;",
			"    sparse::lgs.resize(sparse::n + 1);",
			"    sparse::st.resize(n, vector<Tp>(sparse::lg, 0));",
			"    sparse::op = op;",
			"    sparse::init(a);",
			"  }",
			"",
			"  void init(const vector<Tp>& a) {",
			"    for (int i = 0; i < n; ++i) st[i][0] = a[i], lgs[i + 1] = __lg(i + 1);",
			"    for (int j = 1; j < lg; ++j) {",
			"      for (int i = 0; i + (1 << j) - 1 < n; ++i) {",
			"        st[i][j] = op(st[i][j - 1], st[i + (1 << (j - 1))][j - 1]);",
			"      }",
			"    }",
			"  }",
			"  Tp query(int L, int R) {",
			"    int lg = lgs[R - L + 1];",
